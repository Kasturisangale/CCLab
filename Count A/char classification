%{
#include <stdio.h>
%}

%%
[a-z] {printf("%c is a lowercase character\n", yytext[0]);}
[A-Z] {printf("%c is an uppercase character\n", yytext[0]);}
[^a-zA-Z\n]   {printf("%c is a non-alphabetic character\n", yytext[0]);}
\n {}
%%

int main() {
    printf("Enter the characters:\n");
    yylex();
    return 0;
}
int yywrap() {
    return 1;
}

